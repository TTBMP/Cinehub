<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" stylesheets="@../../styles/stylesLogin.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ttbmp.cinehub.ui.desktop.login.LoginViewController">

   <children>
      <fx:include fx:id="appBar" source="/layout/app_bar/app_bar.fxml"/>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <Label text="Username:">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="usernameLabel">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin></TextField>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <Label text="Password:">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <PasswordField fx:id="passwordLabel">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin></PasswordField>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Button fx:id="loginButton" mnemonicParsing="false" text="Login" textFill="WHITE">
               <HBox.margin>
                  <Insets top="15.0" />
               </HBox.margin></Button>
            <Label fx:id="errorSectionLabel" textFill="#f80000">
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
