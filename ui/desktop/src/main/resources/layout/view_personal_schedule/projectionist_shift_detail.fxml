<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="550.0" spacing="16.0"
      xmlns="http://javafx.com/javafx/16"
      fx:controller="com.ttbmp.cinehub.ui.desktop.viewpersonalschedule.detail.projectionist.ProjectionistShiftDetailViewController">
    <children>
        <VBox>
            <children>
                <HBox spacing="4.0">
                    <children>
                        <Label fx:id="dateLabel" style="-fx-font-size: 16; -fx-font-weight: bold;" text="date">
                            <HBox.margin>
                                <Insets right="8.0"/>
                            </HBox.margin>
                        </Label>
                        <Label fx:id="startLabel" style="-fx-font-size: 16; -fx-font-weight: bold;" text="start"/>
                        <Label style="-fx-font-size: 16; -fx-font-weight: bold;" text="-"/>
                        <Label fx:id="endLabel" style="-fx-font-size: 16; -fx-font-weight: bold;" text="end"/>
                    </children>
                </HBox>
                <HBox spacing="4.0">
                    <children>
                        <Label fx:id="cityLabel" text="city"/>
                        <Label text="as"/>
                        <Label fx:id="roleLabel" text="role"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Separator/>
        <HBox spacing="4.0">
            <Label text="Working in the hall"/>
            <Label fx:id="hallLabel" text="hall"/>
        </HBox>
        <ListView fx:id="projectionListView"/>
        <Separator/>
        <HBox>
            <Button fx:id="backButton" mnemonicParsing="false" text="BACK">
                <styleClass>
                    <String fx:value="btn"/>
                    <String fx:value="btn-secondary"/>
                </styleClass>
            </Button>
            <VBox.margin>
                <Insets top="16.0"/>
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="32.0" left="32.0" right="32.0" top="32.0"/>
    </padding>
</VBox>
