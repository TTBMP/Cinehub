<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="730.0" minWidth="1156.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ttbmp.cinehub.ui.desktop.manageshift.ShowShiftViewController">
    <fx:include fx:id="appBar" source="app_bar/app_bar.fxml" />
    <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="700.0">
        <content>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                <children>
               <HBox fx:id="errorHBox" alignment="CENTER">
                  <children>
                     <Label fx:id="errorLabel" textFill="RED" />
                  </children>
               </HBox>
                    <HBox alignment="CENTER">
                        <children>
                            <Label text="Choice Cinema:" HBox.hgrow="ALWAYS">
                                <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                </HBox.margin>
                            </Label>
                            <ComboBox fx:id="cinemaComboBox">
                                <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                </HBox.margin>
                            </ComboBox>
                        </children>
                    </HBox>
                    <HBox alignment="CENTER" spacing="5.0">
                        <children>
                            <Button fx:id="todayButton" mnemonicParsing="false" text="TODAY" />
                            <Button fx:id="previousButton" mnemonicParsing="false" text="&lt;" />
                            <Button fx:id="nextButton" mnemonicParsing="false" text="&gt;" />
                            <DatePicker fx:id="periodDatePicker" />
                        </children>
                        <VBox.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </VBox.margin>
                    </HBox>
                    <TableView fx:id="shiftTableView" cacheShape="false" centerShape="false" focusTraversable="false" scaleShape="false" VBox.vgrow="ALWAYS">
                        <columns>
                            <TableColumn fx:id="shiftEmployeeTableColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="180.0" prefWidth="-1.0" resizable="false" sortable="false" text="Employee" />
                            <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="-1.0" resizable="false" sortable="false" text="Monday" />
                            <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="-1.0" resizable="false" sortable="false" text="Tuesday" />
                            <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="-1.0" resizable="false" sortable="false" text="Wednesday" />
                            <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="-1.0" resizable="false" sortable="false" text="Thurdsday" />
                            <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="-1.0" resizable="false" sortable="false" text="Friday" />
                            <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="-1.0" resizable="false" sortable="false" text="Saturday" />
                            <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="-1.0" resizable="false" sortable="false" text="Sunday" />
                        </columns>
                        <opaqueInsets>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </opaqueInsets>
                        <VBox.margin>
                            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
                        </VBox.margin>
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                        </columnResizePolicy>
                    </TableView>
                </children>
            </VBox>
        </content>
        <padding>
            <Insets left="16.0" right="16.0" top="16.0" />
        </padding>
    </ScrollPane>
</VBox>
