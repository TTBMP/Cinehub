<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox minHeight="730.0" minWidth="1156.0" stylesheets="@../styles/stylesIvan.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ttbmp.cinehub.ui.desktop.ui.buyticket.payment.PaymentViewController">
    <children>
        <fx:include fx:id="appBar" source="app_bar.fxml" />
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="16.0" VBox.vgrow="ALWAYS">
            <children>
                <HBox alignment="CENTER">
                    <children>
                        <Label alignment="CENTER" text="Enter your billing information " HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="20.0" />
                            </font>
                        </Label>
                    </children>
                    <VBox.margin>
                        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
                    </VBox.margin>
                </HBox>
                <GridPane VBox.vgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="50.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="Name you will find on the ticket:" GridPane.columnIndex="3" />
                        <Label text="Enter the email where the invoice arrives:" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                        <Label text="Credit card expiration date:" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                        <TextField fx:id="nameTextField" alignment="TOP_LEFT" promptText="Insert name" GridPane.columnIndex="4" />
                        <TextField fx:id="emailTextField" alignment="TOP_LEFT" promptText="Insert email" GridPane.columnIndex="4" GridPane.rowIndex="1">
                            <opaqueInsets>
                                <Insets />
                            </opaqueInsets>
                        </TextField>
                        <DatePicker fx:id="fieldExpirationDatePicker" prefHeight="31.0" prefWidth="181.0" promptText="Insert date" GridPane.columnIndex="4" GridPane.rowIndex="2" />
                        <Label text="Surname you will find on the ticket:" />
                        <Label text="Credit card number:" GridPane.rowIndex="1" />
                        <Label text="CVV:" GridPane.rowIndex="2" />
                        <TextField fx:id="surnameTextField" alignment="TOP_LEFT" promptText="Insert surname" GridPane.columnIndex="1" />
                        <TextField fx:id="numberOfCreditCardTextField" alignment="TOP_LEFT" promptText="Insert card" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="cvvTextField" promptText="Insert cvv" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    </children>
                    <VBox.margin>
                        <Insets left="32.0" right="32.0" />
                    </VBox.margin>
                </GridPane>
                <BorderPane>
                    <left>
                        <Button fx:id="returnButton" mnemonicParsing="false" prefHeight="31.0" prefWidth="54.0" text="Back" BorderPane.alignment="CENTER" />
                    </left>
                    <right>
                        <Button fx:id="confirmButton" mnemonicParsing="false" text="Pay" textAlignment="JUSTIFY" BorderPane.alignment="CENTER" />
                    </right>
                    <center>
                        <HBox alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                            <children>
                                <Text fx:id="resultPaymentText" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="214.6708984375" />
                                <Label fx:id="errorSectionLabel" prefHeight="21.0" prefWidth="253.0" textFill="RED" />
                            </children>
                        </HBox>
                    </center>
                    <VBox.margin>
                        <Insets bottom="16.0" left="16.0" right="16.0" />
                    </VBox.margin>
                </BorderPane>
            </children>
        </VBox>
    </children>
</VBox>
