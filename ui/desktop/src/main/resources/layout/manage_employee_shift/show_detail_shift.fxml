<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.lang.String?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
            fx:controller="com.ttbmp.cinehub.ui.desktop.manageshift.detail.ShowShiftDetailViewController">
    <top>
        <VBox alignment="CENTER">
            <Label alignment="CENTER" text="Shift Detail">
                <font>
                    <Font size="25.0"/>
                </font>
            </Label>
            <Separator prefWidth="200.0"/>
        </VBox>
    </top>
    <center>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="309.0" minWidth="10.0"
                                   prefWidth="293.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="451.0" minWidth="10.0"
                                   prefWidth="297.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Name Employee"/>
                <Label fx:id="nameLabel" text="Label" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Surname Employee" GridPane.rowIndex="1"/>
                <Label fx:id="surnameLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Start time:" GridPane.rowIndex="4"/>
                <Label text="End Time:" GridPane.rowIndex="5"/>
                <Label fx:id="hallLabelText" text="Hall:" GridPane.rowIndex="7"/>
                <Label fx:id="cinemaLbl" text="Cinema:" GridPane.rowIndex="6"/>
                <Label fx:id="cinemaLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="startLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="endLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="hallLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Role:" GridPane.rowIndex="2"/>
                <Label fx:id="roleLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Shift Date:" GridPane.rowIndex="3"/>
                <Label fx:id="dateLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            </children>
            <BorderPane.margin>
                <Insets/>
            </BorderPane.margin>
        </GridPane>
    </center>
    <bottom>
        <HBox alignment="CENTER">
            <children>
                <Button fx:id="modifyShiftButton" mnemonicParsing="false" text="Modify Shift">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                    <styleClass>
                        <String fx:value="btn"/>
                        <String fx:value="btn-warning"/>
                    </styleClass>
                </Button>
                <Button fx:id="deleteShiftButton" mnemonicParsing="false" text="Delete Shift">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                    <styleClass>
                        <String fx:value="btn"/>
                        <String fx:value="btn-danger"/>
                    </styleClass>
                </Button>
            </children>
            <BorderPane.margin>
                <Insets top="16.0"/>
            </BorderPane.margin>
        </HBox>
    </bottom>
    <padding>
        <Insets bottom="32.0" left="32.0" right="32.0" top="32.0"/>
    </padding>
</BorderPane>
