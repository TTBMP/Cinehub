<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" xmlns="http://javafx.com/javafx/16"
      fx:controller="com.ttbmp.cinehub.ui.desktop.manageshift.table.CalendarEmployeeItemViewController">
    <ImageView fx:id="employeeImageView" fitHeight="48.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true"/>
    <HBox alignment="CENTER" spacing="4.0">
        <Label fx:id="nameLabel" text="Label"/>
        <Label fx:id="surnameLabel" text="Label"/>
    </HBox>
    <HBox alignment="CENTER" spacing="4.0">
        <Label text="Role:"/>
        <Label fx:id="roleLabel" text="Label"/>
    </HBox>
</VBox>
