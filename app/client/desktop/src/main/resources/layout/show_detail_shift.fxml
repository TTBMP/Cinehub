<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="com.ttbmp.cinehub.app.client.desktop.ui.manageshift.detail.ShowShiftDetailViewController">
    <top>
        <HBox alignment="CENTER">
            <children>
                <Label alignment="CENTER" text="Visualizza Dettagli Turno" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="25.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
    </top>
    <center>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="309.0" minWidth="10.0"
                                   prefWidth="293.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="451.0" minWidth="10.0"
                                   prefWidth="297.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Nome Dipendente:"/>
                <Label fx:id="nameLabel" text="Label" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Cognome Dipendente:" GridPane.rowIndex="1"/>
                <Label fx:id="surnameLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Ora Inizio:" GridPane.rowIndex="4"/>
                <Label text="Ora Fine:" GridPane.rowIndex="5"/>
                <Label fx:id="hallLbl" text="Sala:" GridPane.rowIndex="7"/>
                <Label fx:id="cinemaLbl" text="Cinema:" GridPane.rowIndex="6"/>
                <Label fx:id="cinemaLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="startLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="endLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label fx:id="hallLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </Label>
                <Label text="Ruolo:" GridPane.rowIndex="2"/>
                <Label fx:id="roleLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Data Turno:" GridPane.rowIndex="3"/>
                <Label fx:id="dateLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            </children>
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </BorderPane.margin>
        </GridPane>
    </center>
    <bottom>
        <VBox BorderPane.alignment="TOP_LEFT">
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </BorderPane.margin>
            <children>
                <HBox alignment="TOP_RIGHT">
                    <children>
                        <Button fx:id="modifyShiftButton" mnemonicParsing="false" text="Modifica Turno">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="deleteShiftButton" mnemonicParsing="false" text="Elimina Turno">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </bottom>
</BorderPane>
